@using CommunityService.Models
@model List<Event>

@{
    ViewBag.Title = "Index";
}

<h2>List of Events</h2>

@Model.Count

@foreach (var e in Model)
{
    <h1>@e.Title</h1>
    <p>Event ID: @e.EventID</p>
    <p>@e.Description</p>
    <p>@e.Category</p>
    <p>@e.ExtraInfo</p>
    <p>--------------</p>


}